class Solution {
    public void nextPermutation(int[] nums) {
        int n=nums.length;
        int i=n-1;
        while( i>0 && nums[i] <=nums[i-1] ){
            i--;
        }
        i--;
        
        if(i>=0){
            int j=n-1;
            while (nums[j]<=nums[i]) {
                j--;
                }
            swap(nums,i,j);
        }
        reverse(nums,i+1);
    }
    public void swap(int[] nums,int i,int j){
        int temp=nums[i];
        nums[i]=nums[j];
        nums[j]=temp;
    }
    public void reverse(int[] nums,int i){
        int start=i;
        int end=nums.length-1;
        while(start<end){
            swap(nums,start,end);
            start++;
            end--;
        }
    }
}
